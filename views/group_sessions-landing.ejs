<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/new_navbar.css">
    <link rel="stylesheet" href="/css/login.css" />
    <link rel="stylesheet" href="/css/fix_budget.css" />
    <link rel="stylesheet" href="/css/search-fliter.css">
    <link rel="icon" href="/images/hh-favicon.png">
    <title>Health Highway | Search for Group Sessions</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://apis.google.com/js/api:client.js"></script>
</head>

<body>
    <header>
        <%- include("partials/navbar") %>
    </header>
    <main>
        <!-- Search-Filter (Start) -->
        <div class="grp-session-container">

            <div class="tags-container">
                <button id="fitness" class="tag">Fitness</button>
                <button id="Yoga for kids" class="tag">Hypertension</button>
                <button id="Diabetes" class="tag">Diabetes</button>
                <button id="Obesity" class="tag">Obesity</button>
                <button id="Trending" class="tag">Trending</button>

            </div>
            <div class="trending-classes-container">
                <div class="trending-heading">
                    <img src="/images/trending2.svg" alt="" />
                    <h2>Trending Group Sessions</h2>
                </div>
                <div class="trending-classes">
                    <% sessions.forEach(session => { %>
                        <% if(session.ACTIVENOW){ %>
                            <div style="cursor: pointer;" onclick="location.href='/Group-Yoga-Classes/<%= session._id %>'" class="new-grp-card">
                                <% if(session.PARTICIPANTS>=session.LIMIT){ %>
                                    <div class="booking-status-tag red-tag">Session Over booked</div>
                                <% } %>
                                <img src=<%= session.IMG %> alt="" style="width:100%; height:auto" />
                                <div class="grp-card-txt">
                                <div class="card-title">
                                    <h3><%= session.SESSION_NAME %></h3>
                                    <p>by <%= session.TNAME %></p>
                                </div>
                                <div class="session-time">
                                    <p><%= session.TIME %></p>
                                </div>
                                </div>
                            </div>
                        <%} %>
                        
                    <% }) %>
                </div>
            </div>
            <div class="fitness-yoga-con">
                <div class="yoga-session-heading">
                    <img src="/images/yoga.svg" alt="" />
                    <h2>Upcoming Group Sessions</h2>
                </div>
                <div class="yoga-classes">
                    <% sessions.forEach(session => { %>
                        <% if(!session.ACTIVENOW){ %>
                            <div style="cursor: pointer;" onclick="location.href='/Group-Yoga-Classes/<%= session._id %>'" class="new-grp-card">
                                <% if(session.PARTICIPANTS>=session.LIMIT){ %>
                                    <div class="booking-status-tag red-tag">Session Over booked</div>
                                <% } %>
                                <img src=<%= session.IMG %> alt="" style="width:100%; height:auto" />
                                <div class="grp-card-txt">
                                <div class="card-title">
                                    <h3><%= session.SESSION_NAME %></h3>
                                    <p>by <%= session.TNAME %></p>
                                </div>
                                <div class="session-time">
                                    <p><%= session.TIME %></p>
                                </div>
                                </div>
                            </div>
                        <%} %>
                        
                    <% }) %>
                </div>
            </div>
